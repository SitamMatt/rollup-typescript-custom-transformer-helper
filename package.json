{
  "name": "rollup-typescript-custom-transformer-helper",
  "version": "1.0.0",
  "description": "An utility to apply custom transformers to @rollup/plugin-typescript",
  "scripts": {
    "build": "rollup -c rollup.config.js"
  },
  "keywords": [
    "rollup",
    "typescript",
    "transformers"
  ],
  "author": "sitammatt",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "rollup": "^2.60.1",
    "typescript": "^4.5.2"
  },
  "files": [
    "dist/**/*.*"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "typings": "./dist/esm/index.d.ts",
  "es2015": "./dist/esm/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  }
}
